---
const iconSizes = [32, 48, 64, 96, 128, 192];
const appleIconSizes = [180];
const allSizes = [...iconSizes, ...appleIconSizes];
---

{
    allSizes.map((size) => (
        <link
            rel={iconSizes.includes(size) ? "icon" : "apple-touch-icon"}
            type="image/png"
            sizes={`${size}x${size}`}
            href={`/favicons/favicon-dark-${size}.png`}
        />
    ))
}
