---
import PageLayout from "../layouts/PageLayout.astro";

const links = [
                ['https://github.com/SnowViktor', "Personal GitHub"],
                ['https://github.com/SnowViktor/Website', 'Website GitHub']
              ];
---

<PageLayout title="關於" description="【SnowViktor和網站的資訊】：Personal GitHub | Website GitHub">
    <ul>
        {links.map(link => <li><a href={link[0]} target="_blank">{link[1]}</a></li>)}
    </ul>
</PageLayout>